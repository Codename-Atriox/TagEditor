<UserControl x:Class="TagEditor.UI.Interfaces.Editor.Params.EnumParam"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TagEditor.UI.Interfaces.Editor.Params"
             mc:Ignorable="d" 
             Height="18"
             d:DesignHeight="18" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <TextBlock x:Name="Namebox" Text="SampleParamName" Foreground="#FFD6D6D6" Margin="0,0,5,0" />
        <Grid Grid.Column="1">
            <ComboBox x:Name="enumbox" SelectionChanged="enumbox_SelectionChanged">
                
            </ComboBox>
        </Grid>
    </Grid>
</UserControl>
