<Window x:Class="TagEditor.UI.Windows.ErrorWind"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TagEditor.UI.Windows"
        mc:Ignorable="d"
        Title="ErrorWind" Height="450" Width="800">
    <Grid Background="#FF2F2F2F">
        <StackPanel Margin="5,0,0,0">
            <TextBlock x:Name="e_context" Text="context" Foreground="#FFD6D6D6" FontSize="20"/>
            <TextBlock x:Name="e_message" Text="Error Message" Foreground="#FFD6D6D6"/>
            <TextBlock x:Name="e_source" Text="Error Source" Foreground="#FFD6D6D6"/>
            <TextBlock x:Name="e_stacktrace" Text="Error Stack Trace" Foreground="#FFD6D6D6"/>
        </StackPanel>
    </Grid>
</Window>
